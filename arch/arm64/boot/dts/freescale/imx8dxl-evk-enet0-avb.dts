/dts-v1/;

#include "imx8dxl-evk-enet0.dts"

/* AVB HW timer*/
&gpt5 {
	compatible = "fsl,avb-gpt";
	timer-channel = <1>; /* Use output compare channel 1*/
	prescale = <1>;
	domain = <0>;

	interrupts = <GIC_SPI 268 IRQ_TYPE_LEVEL_HIGH>;

	status = "okay";
};
